<!-- BEGIN HEADER -->
<div class="header">
  <div class="container">
    <!--<a class="site-logo" href="<%- url_for() %>" id="logo"><%= config.title %></a>-->

    <a class="site-logo" href="/">
      <img src="<%- url_for('assets/img/logo.png') %>" alt="KDE Indonesia">
    </a>

    <a href="javascript:void(0);" class="mobi-toggler"><i class="fa fa-bars"></i></a>

    <!-- BEGIN NAVIGATION -->
    <div class="header-navigation pull-right font-transform-inherit">
      <ul>
	<% for (var i in theme.menu){ %>
	<li class="<%= url_for(theme.menu[i]) == url_for(path).substring(0, url_for(path).lastIndexOf('/') + 1) ? 'active' : ''   %>">
	  <a  href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
	</li>
	<% } %>
	<!-- BEGIN TOP SEARCH -->
	<li class="menu-search">
	  <span class="sep"></span>
	  <i class="fa fa-search search-btn"></i>
	  <div class="search-box">
	    <form id="search-form">
				<input  type="text" name="q" id="tipue_search_input" autocomplete="off" required placeholder="Type, Enter and Search" class="form-control st-default-search-input" />
			</form>
			<div id="tipue_search_content"></div>
		</div> 
	</li>
	<!-- END TOP SEARCH -->
      </ul>
    </div>
    <!-- END NAVIGATION -->
  </div>
</div>
<!-- Header END -->
